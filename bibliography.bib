@article{szaboMotherOffspringAssociation2008,
  title = {Mother\textendash Offspring Association in the Humpback Whale, {{Megaptera}} Novaeangliae: Following Behaviour in an Aquatic Mammal},
  shorttitle = {Mother\textendash Offspring Association in the Humpback Whale, {{Megaptera}} Novaeangliae},
  author = {Szabo, Andy and Duffus, Dave},
  year = {2008},
  month = mar,
  journal = {Animal Behaviour},
  volume = {75},
  number = {3},
  pages = {1085--1092},
  issn = {0003-3472},
  doi = {10.1016/j.anbehav.2007.08.019},
  abstract = {We observed 42 humpback whale mother\textendash offspring pairs on a foraging ground in southeast Alaska during 2001 and 2002 to examine their spatial association. We found that whenever these pairs were at the surface they maintained close proximity similar to that observed in terrestrial followers. By examining their dive behaviour, however, we determined that calves dive significantly less frequently and for shorter durations than their mothers, leading to periods of separation during foraging dives. Because this is likely to increase the risk of calf predation, females responded by significantly shortening the duration of their dives when unaccompanied by their offspring, thereby reducing this risk. As the season progressed, we observed that this behaviour waned and females became significantly less responsive to their offspring. At the same time, calves appeared to increase their role in maintaining proximity by synchronizing their dives with the mothers increasingly often. We discuss this shift in proximity maintenance in terms of the energetics associated with lactation, increased foraging effort by the offspring and parent\textendash offspring conflict.},
  keywords = {dive,follower,humpback whale,maternal behaviour,Megaptera novaeangliae},
  file = {/home/anon/Zotero/storage/7PEFRT5X/Szabo och Duffus - 2008 - Motherâ€“offspring association in the humpback whale.pdf;/home/anon/Zotero/storage/AN82WQVX/S0003347207005374.html}
}

