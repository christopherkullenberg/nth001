@book{bernfortQALYSomEffektmatt2012,
  title = {{{QALY}} Som Effektm\aa tt Inom V\aa rden [{{Elektronisk}} Resurs] : {{M\"ojligheter}} Och Begr\"ansningar},
  author = {Bernfort, Lars},
  year = {2012},
  publisher = {{Link\"oping University Electronic Press}},
  address = {{Link\"oping}},
  abstract = {H\"also- och sjukv\aa rdens resurser \"ar \"andliga och f\"or att anv\"anda resurserna p\aa{} b\"asta s\"att \"ar prioriteringar n\"odv\"andiga. I syfte att tj\"ana som underlag vid prioriteringar genomf\"ors allt fler h\"alsoekonomiska utv\"arderingar, i vilka kostnader f\"or olika behandlingar st\"alls mot de positiva effekter p\aa{} h\"alsa/livskvalitet som de medf\"or. Dessa positiva effekter uttrycks vanligen som kvalitetsjusterade levnads\aa r (QALYs) och resultatet av en utv\"ardering~ uttrycks som kostnaden per vunnen QALY. Det \"ar dock inte sj\"alvklart vad en~ QALY st\aa r f\"or d\aa{} denna kan m\"atas och r\"aknas fram p\aa{} flera olika s\"att.Denna rapport \"agnas \aa t effektm\aa ttet QALY med avseende p\aa{} vad det inneb\"ar, vad det \"ar t\"ankt att representera, hur det m\"ats, teoretiska och metodologiska problem, samt t\"ankbara alternativa tillv\"agag\aa ngss\"att. F\"orutom att beskriva och ge en bild av effektm\aa ttet QALY \"ar syftet med rapporten att identifiera intressanta och relevanta fr\aa gor f\"or vidare forskning om QALY och m\"atning av v\aa rdens effekter.Rapporten inleds med ett kapitel d\"ar QALY-ansatsen s\"atts in i sitt sammanhang genom en bakgrund och teoretisk grund. Sedan f\"oljer en redog\"orelse f\"or prospekt teorin, som tagits fram som en kritik mot den traditionella QALYansatsen och som m\"ojligen kan utg\"ora en alternativ ansats. Kapitel 2 beskriver fr\aa n vilka olika perspektiv QALY-vikter kan m\"atas samt vilken betydelse val av perspektiv kan ha f\"or resultatet. Med perspektiv avses h\"ar i f\"orsta hand vem som ska tillfr\aa gas om hur bra eller d\aa ligt ett sjukdomstillst\aa nd \"ar, drabbade patienter eller representanter f\"or allm\"anheten? I kapitel 3 beskrivs metoder f\"or m\"atning och v\"ardering av QALY-vikter. S\aa v\"al direkta som indirekta metoder f\"orklaras. Metodproblem beskrivs och diskussion f\"ors om f\"ordelar och nackdelar med olika m\"atmetoder samt vilka skillnader i resultat de kan f\"orv\"antas medf\"ora. Kapitel 4 \"agnas \aa t specialfr\aa gorna om negativa QALY-vikter, eller h\"alsotillst\aa nd v\"arre \"an d\"oden, samt v\"ardering av tempor\"ara tillst\aa nd. M\"atmetoder f\"or dessa situationer och problem f\"orknippade med dessa diskuteras. I kapitel 5 s\"atts QALY-ansatsen in i ett st\"orre sammanhang genom att den relateras till id\'en om r\"attvisa. Fr\aa gor som diskuteras \"ar till exempel huruvida QALY-ansatsen \"ar f\"orenlig med en r\"attvis f\"ordelning av resurser och om r\"attvisa borde tas h\"ansyn till i QALY-modellen eller inte. I kapitel 6 l\"ankas QALY-ansatsen till teorier om lycka. Kan lyckoforskningen berika den traditionella h\"alsoekonomiska ansatsen? Kan lyckoforskningen bidra till b\"attre metoder f\"or m\"atning av v\aa rdens effekter, det vi vanligen kallar QALY? Rapporten avslutas med en diskussion som mynnar ut i intressanta och angel\"agna forskningsfr\aa gor. Tanken \"ar att vi ska g\aa{} vidare med en del av dessa fr\aa gest\"allningar.}
}

@article{bessiScienceVsConspiracy2015,
  title = {Science vs {{Conspiracy}}: {{Collective Narratives}} in the {{Age}} of {{Misinformation}}},
  shorttitle = {Science vs {{Conspiracy}}},
  author = {Bessi, Alessandro and Coletto, Mauro and Davidescu, George Alexandru and Scala, Antonio and Caldarelli, Guido and Quattrociocchi, Walter},
  year = {2015},
  month = feb,
  journal = {PLOS ONE},
  volume = {10},
  number = {2},
  pages = {e0118093},
  issn = {1932-6203},
  doi = {10.1371/journal.pone.0118093},
  abstract = {The large availability of user provided contents on online social media facilitates people aggregation around shared beliefs, interests, worldviews and narratives. In spite of the enthusiastic rhetoric about the so called  collective intelligence  unsubstantiated rumors and conspiracy theories\textemdash e.g., chemtrails, reptilians or the Illuminati\textemdash are pervasive in online social networks (OSN). In this work we study, on a sample of 1.2 million of individuals, how information related to very distinct narratives\textemdash i.e. main stream scientific and conspiracy news\textemdash are consumed and shape communities on Facebook. Our results show that polarized communities emerge around distinct types of contents and usual consumers of conspiracy news result to be more focused and self-contained on their specific contents. To test potential biases induced by the continued exposure to unsubstantiated rumors on users' content selection, we conclude our analysis measuring how users respond to 4,709 troll information\textemdash i.e. parodistic and sarcastic imitation of conspiracy theories. We find that 77.92\% of likes and 80.86\% of comments are from users usually interacting with conspiracy stories.},
  keywords = {AIDS,Facebook,Graphs,Imitation,Italian people,Random variables,social media,Social networks},
  file = {/home/anon/Zotero/storage/GXPUX6WP/Bessi et al. - 2015 - Science vs Conspiracy Collective Narratives in th.pdf;/home/anon/Zotero/storage/T25HSX2N/article.html}
}

