  booktitle = {Language {{Teaching}} in the {{Linguistic Landscape}}: {{Mobilizing Pedagogy}} in {{Public Space}}},
  author = {Nielsen, Helle Lykke and Rosendal, Tove and J{\"a}rlehed, Johan and Kullenberg, Christopher},
