@book{hansenMetodologiskaPerspektivPa2009,
  title = {Metodologiska Perspektiv P\aa{} Konstnads-Nyttoanalys [{{Elektronisk}} Resurs]},
  author = {Hansen, Fredrik},
  year = {2009},
  publisher = {{Statens v\"ag- och transportforskningsinstitut}},
  address = {{Stockholm}},
  abstract = {Inom samh\"allsekonomiska analyser utg\"or kostnad-nyttoanalys (CBA) ofta en viktig funktion och detta g\"aller s\"arskilt vid dagens transportpolitiska beslutsfattande. Samtidigt \"ar det en metod som \"ar i centrum f\"or en del diskussioner. F\"oljande uppsats avser att analysera dessa diskussioner ur ett ekonomisk metodologiskt perspektiv. Tre arenor kommer att beaktas: filosofers kritik, den inomdisciplin\"ara debatten bland nationalekonomer samt den svenska samh\"allsdebatten. I huvudsak n\aa s slutsatsen att det \"ar blandad kvalit\'e i s\aa v\"al kritik mot CBA som dess f\"orsvar. Den filosofiska kritiken m\aa ste inh\"amtas \"aven om den inte sl\aa r fullt mot CBA som samh\"allsekonomisk metod. CBA-anh\"angare \"ar medvetna om delar av problematiken och jobbar med det, fr\"amst kring v\"arderingsstudier. Dock kan detta prioriteras \"annu mer fram\"over. I den svenska samh\"allsdebatten f\"ors det fram kritik som inte n\aa r fram. Allt detta sammantaget tillsammans med avsaknaden av alternativ inneb\"ar att CBA f\aa r ses som l\"amplig i nul\"aget. Framtida forskning b\"or s\"arskilt utreda interaktionen mellan CBA och beteendeekonomi samt lyckoforskning. Det \"ar ocks\aa{} l\"ampligt att betrakta CBA utifr\aa n vilken funktion det har inom ett beslutsunderlag. S\"arskilt intressant \"ar d\aa{} att vidare studera V\"agverkets "samlad effektbed\"omning".}
}

